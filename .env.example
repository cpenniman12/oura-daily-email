# Oura Ring API
OURA_ACCESS_TOKEN=your_oura_access_token_here

# Email Configuration
# For Gmail, use smtp.gmail.com and port 587
# You'll need to create an App Password: https://myaccount.google.com/apppasswords
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SENDER_EMAIL=your_email@gmail.com
SENDER_PASSWORD=your_app_password_here

# Recipient email
RECIPIENT_EMAIL=your_email@gmail.com

# Schedule (24-hour format)
SCHEDULE_TIME=10:00

